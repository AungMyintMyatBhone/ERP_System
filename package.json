{
  "name": "erp-system-fullstack",
  "version": "1.0.0",
  "description": "A comprehensive ERP system built with React frontend and Node.js backend",
  "main": "server/server.js",
  "scripts": {
    "start": "cd server && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install-server": "cd server && npm install",
    "heroku-postbuild": "npm install && npm run build && cd server && npm install",
    "render-build": "npm install && CI=false npm run build && cd server && npm install"
  },
  "keywords": [
    "erp",
    "react",
    "management",
    "business"
  ],
  "author": "Totals Inc",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "@mui/material": "^5.11.0",
    "@mui/icons-material": "^5.11.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "axios": "^1.3.0",
    "recharts": "^2.5.0",
    "date-fns": "^2.29.0",
    "concurrently": "^7.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}